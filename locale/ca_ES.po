msgctxt "error:account.invoice-account.invoice.milestone:"
msgid "The Invoice must be unique."
msgstr "La factura ha de ser única."

msgctxt "error:account.invoice-account.invoice.milestone:"
msgid "The Milestone must be unique."
msgstr "La fita ha de ser única."

msgctxt "error:account.invoice.milestone.group.type:"
msgid "Invalid line \"%(line)s\" in payment term \"%(term)s\"."
msgstr "La línia \"%(line)s\" del termini de pagament \"%(term)s\" no és correcte."

msgctxt "error:account.invoice.milestone.group.type:"
msgid "Last line of payment term \"%s\" must be of type remainder."
msgstr ""
"L'última línia del termini de pagament \"%s\" ha de ser de tipus romanent."

msgctxt "error:account.invoice.milestone.group.type:"
msgid "Missing remainder line in payment term \"%s\"."
msgstr "Falta el remanent de la línia del termini de pagament \"%s\"."

msgctxt "error:account.invoice.milestone.group:"
msgid ""
"An advancement product must be defined in order to generate advancement "
"invoices.Please define one in account configuration."
msgstr ""
"S'ha de definir un producte d'avançament per a generar factures "
"d'avançament. Sisplau, definiu-n'hi un a la configuració financera."

msgctxt "error:account.invoice.milestone.group:"
msgid ""
"Can not delete milestone group \"%s\" because it's milestone \"%s\" is not "
"in draft or cancel state."
msgstr ""
"No es pot eliminar el grup de fites \"%S\" perquè conté la fita \"%s\" que "
"no està en estat esborrany o cancel·lat."

msgctxt "error:account.invoice.milestone.group:"
msgid ""
"There is no milestone sequencedefined. Please define one in account "
"configuration"
msgstr ""
"No hi ha cap seqüencia de fites definida. Si us plau, definiu-n'hi una a la "
"configuració comptable."

msgctxt "error:account.invoice.milestone.type:"
msgid "Day of month must be between 1 and 31."
msgstr "El dia del mes ha d'estar entre 1 i 31."

msgctxt "error:account.invoice.milestone.type:"
msgid ""
"Percentage and Divisor values are not consistent in line \"%(line)s\" of "
"payment term \"%(milestone)s\"."
msgstr ""
"Els valors del percentatge i el division no son consistents a la línia "
"\"%(line)s\" del grup de tipus de fites \"%(milestone)s\"."

msgctxt "error:account.invoice.milestone:"
msgid ""
"Milestone \"%(milestone)s\" can not be invoiced because its move \"%(move)s "
"is not done."
msgstr ""
"La fita \"%(milestone)s\" no es pot facturar perquè el seu movimient "
"\"%(move)s\" no està realitzat."

msgctxt "error:account.invoice.milestone:"
msgid ""
"There is no milestone sequencedefined. Please define one in account "
"configuration"
msgstr ""
"No hi ha cap seqüencia de fites definida. Si us plau, definiu-n'hi una a la "
"configuració comptable."

msgctxt "error:account.invoice:"
msgid ""
"Amount of invoice \"%(invoice)s\" must be equal than its milestone "
"\"%(milestone)s\" amount"
msgstr ""
"El import de la factura \"%(invoice)s\" ha de ser igual a l'import de la "
"seva fita \"%(milestone)s\"."

