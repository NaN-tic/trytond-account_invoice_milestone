msgctxt "error:account.invoice-account.invoice.milestone:"
msgid "The Invoice must be unique."
msgstr "La factura debe ser única."

msgctxt "error:account.invoice-account.invoice.milestone:"
msgid "The Milestone must be unique."
msgstr "El hito debe ser único."

msgctxt "error:account.invoice.milestone.group.type:"
msgid "Invalid line \"%(line)s\" in payment term \"%(term)s\"."
msgstr "La línea \"%(line)s\" del plazo de pago \"%(term)s\" no es correcta."

msgctxt "error:account.invoice.milestone.group.type:"
msgid "Last line of payment term \"%s\" must be of type remainder."
msgstr "La última línea del plazo de pago \"%s\" debe ser de tipo remanente."

msgctxt "error:account.invoice.milestone.group.type:"
msgid "Missing remainder line in payment term \"%s\"."
msgstr "Falta la línea de remanente en el plazo de pago \"%s\"."

msgctxt "error:account.invoice.milestone.group:"
msgid ""
"An advancement product must be defined in order to generate advancement "
"invoices.Please define one in account configuration."
msgstr ""
"Se debe definir un producto de avances de hitos para generar una factura por"
" avanzado. Por favor, defina uno en la configuración financiera."

msgctxt "error:account.invoice.milestone.group:"
msgid ""
"Can not delete milestone group \"%s\" because it's milestone \"%s\" is not "
"in draft or cancel state."
msgstr ""
"No se puede eliminar el grupo de fitas \"%s\" porqué la fita \"%s\" no está "
"en estado borrador o cancelado."

msgctxt "error:account.invoice.milestone.group:"
msgid ""
"There is no milestone sequencedefined. Please define one in account "
"configuration"
msgstr ""
"No se ha definido ninguna secuencia de hitos. Por favor, defina uno en la "
"configuración contable."

msgctxt "error:account.invoice.milestone.type:"
msgid "Day of month must be between 1 and 31."
msgstr "El día del mes debe estar entre 1 y 31."

msgctxt "error:account.invoice.milestone.type:"
msgid ""
"Percentage and Divisor values are not consistent in line \"%(line)s\" of "
"payment term \"%(milestone)s\"."
msgstr ""

msgctxt "error:account.invoice.milestone:"
msgid ""
"Milestone \"%(milestone)s\" can not be invoiced because its move \"%(move)s "
"is not done."
msgstr ""
"No se puede facturar el hito \"%(milestone)s\" porque su movimiento "
"\"%(move)s\" no esta realizado."

msgctxt "error:account.invoice.milestone:"
msgid ""
"There is no milestone sequencedefined. Please define one in account "
"configuration"
msgstr ""
"No se ha definido ninguna secuencia de hitos. Por favor, defina uno en la "
"configuración contable."

msgctxt "error:account.invoice:"
msgid ""
"Amount of invoice \"%(invoice)s\" must be equal than its milestone "
"\"%(milestone)s\" amount"
msgstr ""
"El importe de la factura \"%(invoice)s\" debe ser igual al importe de su "
"hito \"%(milestone)s\"."

